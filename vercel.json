{
  "version": 2,
  "name": "carbonecomarket-invest",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "."
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "buildCommand": "echo 'Static site - no build needed' && ls -la",
  "outputDirectory": ".",
  "installCommand": "echo 'No dependencies to install'"
}
